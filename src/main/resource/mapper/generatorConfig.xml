<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--数据库驱动jar -->
    <classPathEntry
            location="C:/Users/sen.shao/.m2/repository/com/oracle/ojdbc6/11.2.0.1.0/ojdbc6-11.2.0.1.0.jar" />

    <context id="mysql2Dto" targetRuntime="MyBatis3">
        <!--去除注释 -->
        <commentGenerator
                type="org.mybatis.generator.internal.DefaultCommentGenerator">
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!--数据库连接 -->
        <jdbcConnection driverClass="oracle.jdbc.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:orcl"
                        userId="test" password="123456">
        </jdbcConnection>
        <!--默认false Java type resolver will always use java.math.BigDecimal if
            the database column is of type DECIMAL or NUMERIC. -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>


        <!-- *******************我是分割线******************* -->

        <!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
        <javaModelGenerator
                targetPackage="com.bandweaver.tunnel.common.biz.pojo"
                targetProject="D:\workspace\MaxTunnel\MaxTunnel-Common\MaxTunnel-Common-Biz\src\main\java\">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!--生成xxxMapper.xml文件 -->
        <sqlMapGenerator
                targetPackage="com.bandweaver.tunnel.dao.common"
                targetProject="D:\workspace\MaxTunnel\MaxTunnel-dao\src\main\java\">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        <!--生成xxxMapper.java接口 可以配置 type="XMLMAPPER"生成xml的dao实现 context id="DB2Tables" 修改targetRuntime="MyBatis3" -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.bandweaver.tunnel.dao.common"
                             targetProject="D:\workspace\MaxTunnel\MaxTunnel-dao\src\main\java\">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        <!-- 指定生成的表 -->
        <table tableName="t_common_file_info"
               domainObjectName="FileInfo" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>

